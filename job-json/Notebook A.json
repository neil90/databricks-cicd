{
    "name":"Notebook A",
    "new_cluster":{
       "spark_version":"5.5.x-scala2.11",
       "aws_attributes":{
          "zone_id":"us-west-2b",
          "first_on_demand":1,
          "availability":"SPOT_WITH_FALLBACK"
       },
       "node_type_id":"i3.xlarge",
       "spark_env_vars":{
          "PYSPARK_PYTHON":"/databricks/python3/bin/python3"
       },
       "enable_elastic_disk":false,
       "num_workers":8
    },
    "email_notifications":{
 
    },
    "timeout_seconds":0,
    "notebook_task":{
       "notebook_path":"/production/Notebook A"
    },
    "max_concurrent_runs":1
 }